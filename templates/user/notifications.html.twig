{% extends 'base.html.twig' %}
{% block title %}
	1234
{% endblock %}
{% block body %}
	<h1>Hello
		{{ user_first_name }}!</h1>
	<p>You have
		{{ notifications|length }}
		new notifications.</p>

	<table class="table table-hover">
		<tr>
			<th>Company</th>
			<th>Contact</th>
		</tr>
		{% for notification in notifications %}
			<tr>
				<td>{{ notification.first_name }}</td>
				<td>{{ notification.last_name }}</td>
			</tr>
		{% endfor %}
	</table>
{% endblock %}
